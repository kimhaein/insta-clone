extends layouts/main
include mixins/contentBlock

block content
    h1 나는 콘텐츠
    div 
    a(href='/') 홈
    a(href='/login') 로그인
    a(href='/join') 회원가입
    a(href='/users/1') 회원상세
    each item in contents
        +contentBlock({
            idx: item.idx,
            creator: item.creator,
            content: item.fileUrl,
            date: item.regdate
        })